{
  "diagnostic.errorSign": "E",
  "diagnostic.warningSign": "W",
  "diagnostic.infoSign": "I",
  "diagnostic.hintSign": "H",
  "languageserver": {
    "golang": {
      "command": "go-langserver",
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    },
    "metals": {
      "command": "metals-vim",
      "rootPatterns": [
        "build.sbt"
      ],
      "filetypes": [
        "scala",
        "sbt"
      ]
    },
    "rust": {
      "command": "rustup run' nightly rls",
      "filetypes": [
        "rust"
      ],
      "initializationOptions": {}
    },
    "kotlin": {
      "command": "~/github/KotlinLanguageServer/server/build/install/server/bin/server",
      "filetypes": [
        "kotlin"
      ],
      "initializationOptions": {}
    },
    "dart": {
      "command": "dart",
      "args": [
        "/usr/lib/dart/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp"
      ],
      "filetypes": [
        "dart"
      ],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    }
  }
}
