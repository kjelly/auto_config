{
  "Lua": {
    "diagnostics": {
      "globals": [
        "vim"
      ]
    },
    "workspace.library": {
      "/opt/neovim.nightly/usr/share/nvim/runtime/lua": true,
      "/opt/neovim.nightly/usr/share/nvim/runtime/lua/vim": true,
      "/opt/neovim.nightly/usr/share/nvim/runtime/lua/vim/lsp": true
    }
  },
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "dart",
    "javascript",
    "json"
  ],
  "coc.preferences.messageLevel": "error",
  "coc.preferences.snippetStatusText": "üå†",
  "coc.source.around.firstMatch": false,
  "coc.source.around.priority": 95,
  "coc.source.buffer.firstMatch": false,
  "coc.source.buffer.priority": 90,
  "coc.source.word.priority": 20,
  "diagnostic.errorSign": "‚ùå",
  "diagnostic.hintSign": "üí°",
  "diagnostic.infoSign": "üí¨",
  "diagnostic.virtualText": false,
  "diagnostic.warningSign": "üî∫",
  "flutter.lsp.initialization.onlyAnalyzeProjectsWithOpenFiles": false,
  "flutter.openDevLogSplitCommand": "botright 12split",
  "git.changeRemovedSign.text": "~",
  "json.enable": true,
  "json.format.enable": true,
  "languageserver": {
    "efm": {
      "args": [],
      "command": "efm-langserver",
      "filetypes": [
        "vim",
        "eruby",
        "markdown",
        "yaml"
      ]
    },
    "golang": {
      "command": "go-langserver",
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "diagnosticsEnabled": true,
        "gocodeCompletionEnabled": true,
        "lintTool": "golint"
      }
    },
    "kotlin": {
      "command": "~/github/KotlinLanguageServer/server/build/install/server/bin/server",
      "filetypes": [
        "kotlin"
      ],
      "initializationOptions": {}
    },
    "metals": {
      "command": "metals-vim",
      "filetypes": [
        "scala",
        "sbt"
      ],
      "rootPatterns": [
        "build.sbt"
      ]
    },
    "rust": {
      "command": "rustup run' nightly rls",
      "filetypes": [
        "rust"
      ],
      "initializationOptions": {}
    }
  },
  "list.insertMappings": {
    "<A-n>": "normal:j",
    "<A-p>": "normal:k",
    "<C-f>": "feedkeys:\\<C-f>",
    "<C-g>": "feedkeys:\\<C-b>",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-r>": "do:refresh",
    "<C-t>": "action:tabe",
    "<C-v>": "eval:@@"
  },
  "list.normalMappings": {
    "c": "expr:MyExprFunc",
    "d": "action:delete"
  },
  "list.source.files.defaultOptions": [
    "--number-select"
  ],
  "pyright.organizeimports.provider": "isort",
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pytypeEnabled": true,
  "suggest.autoTrigger": "always",
  "suggest.detailField": "preview",
  "suggest.detailMaxLength": 60,
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.lowPrioritySourceLimit": 10,
  "suggest.maxPreviewWidth": 100,
  "suggest.noselect": true,
  "suggest.numberSelect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.removeDuplicateItems": true,
  "suggest.snippetIndicator": "",
  "suggest.timeout": 5000,
  "suggest.triggerCompletionWait": 100,
  "tabnine.priority": 90
}
