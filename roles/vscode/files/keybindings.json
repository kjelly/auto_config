[
    { "key": "alt+e",  "command": "workbench.action.showCommands"},
    { "key": "ctrl+3", "command": "workbench.view.explorer"},
    { "key": "ctrl+4", "command": "workbench.view.search"},
    { "key": "ctrl+5", "command": "workbench.view.git"},
    { "key": "ctrl+6", "command": "workbench.view.debug"},
    { "key": "ctrl+n", "command": "workbench.action.toggleSidebarVisibility"},
    { "key": "alt+;", "command": "workbench.action.terminal.toggleTerminal"},
    { "key": "alt+'", "command": "workbench.action.terminal.focus", "when": "!terminalFocus"},
    { "key": "alt+'", "command": "workbench.action.focusFirstEditorGroup", "when": "terminalFocus"},
    { "key": "alt+p", "command": "workbench.action.terminal.focusPrevious", "when": "terminalFocus"},
    { "key": "alt+n", "command": "workbench.action.terminal.focusNext", "when": "terminalFocus"},
    { "key": "alt+c", "command": "workbench.action.terminal.open", "when": "terminalFocus"},
    { "key": "alt+k", "command": "workbench.action.terminal.kill", "when": "terminalFocus"},
    { "key": "alt+b", "command": "workbench.action.terminal.scrollUp", "when": "terminalFocus"},
    { "key": "alt+f", "command": "workbench.action.terminal.scrollDown", "when": "terminalFocus"},
    { "key": "alt+d", "command": "editor.action.showHover", "when": "editorTextFocus"},
    { "key": "alt+/", "command": "editor.action.triggerSuggest", "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"},
    { "key": ", q",              "command": "workbench.action.closeActiveEditor",
                                   "when": "editorTextFocus && !inDebugRepl && vim.mode == 'Normal'" },
    { "key": ", f",              "command": "workbench.files.action.showActiveFileInExplorer",
                                   "when": "editorTextFocus && !inDebugRepl && vim.mode == 'Normal'" },
    { "key": "j", "command": "list.focusDown", "when": "filesExplorerFocus" }, 
    { "key": "k", "command": "list.focusUp", "when": "filesExplorerFocus" }, 
    { "key": "l", "command": "list.expand", "when": "filesExplorerFocus" }, 
    { "key": "h", "command": "list.collapse", "when": "filesExplorerFocus" }, 
    { "key": "enter", "command": "list.select", "when": "filesExplorerFocus" },
    { "key": "ctrl+a", "command": "editor.action.selectAll"},

    { "key": "alt+h", "command": "workbench.action.moveEditorToPreviousGroup" },
    { "key": "alt+j", "command": "workbench.action.focusFirstEditorGroup" },
    { "key": "alt+k", "command": "workbench.action.focusSecondEditorGroup" },
    { "key": "alt+l", "command": "workbench.action.moveEditorToNextGroup" },
    { "key": "ctrl+w", "command": "workbench.action.closeActiveEditor" }
]